<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_Term_Days__c</fullName>
    <description>Calculates the contract mid-point</description>
    <externalId>false</externalId>
    <formula>Contract_Start_Date__c +(Contract_End_Date__c -  Contract_Start_Date__c)/2</formula>
    <inlineHelpText>Calculates the contract mid-point</inlineHelpText>
    <label>Contract Decision Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
